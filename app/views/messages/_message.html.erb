<div class=“message”>
  <% if message.user_id.nil? %>
    <div class="success message">
      <!-- <h6><%= 'Web User' %></h6> -->
      <p><%= message.content %></p>
    </div>
  <% else %>
    <div class="info message">
      <h6><%= User.find_by(telegram_id: message.user_id).first_name + ' ' +
      User.find_by(telegram_id: message.user_id).last_name%></h6>
      <p><%= message.content %></p>
    </div>
  <% end %>
</div>
